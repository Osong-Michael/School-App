<div class="container">
  <div class="row center sign-up-form">
    <div class="col s12 m12 l12">
      <div class="card-panel">
        <h3 class="header">Log In</h3>
        <div class="row">
          <%= form_for(:logins, html: {class: "col s12"}) do |f| %>
            <div class="row">
              <div class="input-field col s12">
                <%= f.email_field :email, placeholder: 'Enter email', class: 'validate', autofocus: true %>
                <%= f.label :email, class: 'active' %>
              </div>
            </div>
            <div class="row">
              <div class="input-field col s12">
                <%= f.password_field :password, class: 'validate', placeholder: 'Enter password' %>
                <%= f.label :password, class: 'active' %>
              </div>
            </div>
            

            <div class="row">
              <div class="input-field col s12">
                <%= f.button "Log In", class: 'btn teal lighten-1 waves-effect waves-light' %>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>